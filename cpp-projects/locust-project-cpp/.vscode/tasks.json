{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: Build the locust-project-cpp Project",
			"command": "/usr/bin/g++",
			"args": [
				"-g",
				"-o",
				"${workspaceFolder}/Release/locust",				
				"${workspaceFolder}/src/*.cpp"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}